    </Content>
    <Content Update="wwwroot\video\EPS_Demo_Input_Supplemental.wmv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
    <Content Update="wwwroot\video\EPS_Intro_Powerpoint_Session.wmv">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Content>
  </ItemGroup>

</Project>
